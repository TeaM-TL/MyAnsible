[libdefaults]
        default_realm = {{ domain_name | upper }}
        dns_lookup_realm = false
        dns_lookup_kdc = true

[realms]
{{ domain_name | upper }} = {
        kdc = {{ domain_name }}
        default_domain = {{ domain_name }}
}

